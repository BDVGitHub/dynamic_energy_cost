{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "baseBranches": ["main"],
  "rebaseWhen": "conflicted",
  "labels": ["dependencies"],
  "matchUpdateTypes": ["minor", "patch", "digest"],
  "automergeStrategy": "merge-commit",
  "prHourlyLimit": 2
}
{
  "dependencyDashboard": true
}
