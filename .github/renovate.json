{
  "baseBranches": ["main"],
  "rebaseWhen": "conflicted",
  "commitMessage": "Update dependencies",
  "labels": ["dependencies"],
  "automergeStrategy": "merge-commit",
  "dependencyDashboard": false,
  "prBodyTemplate": "{{{header}}}{{{table}}}{{{warnings}}}{{{notes}}}{{{changelogs}}}",
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "digest"]
    }
  ]
}
